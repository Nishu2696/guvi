function sum(a,b){
    return (a+b);
}

function whole(){
    let a = (Math.random()*10);
    return a;
}

function random_sum(a,b){
    return (a+b);
}

let a =5;
let b=10;
let c=prompt();
let e=parseInt();
let d=Math.random();
console.log(sum(a,b));
console.log(whole());
console.log(random_sum(e,d));