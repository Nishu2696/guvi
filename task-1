#1. write a function to generate random whole number between 0 to 10
#2. write a function which will get two parameters and return a sum of it
#3. write a program to create a calculator with all airthematic operations has functions
#4. do the following:
    #a. create a function to sum two numbers and return the value
    #b. create a function to generate random number between 0 to 10
    #c. pass two values to the sum function, but first value should be passed by the user and the second value should be generated by a random function

#1. write a function to generate random whole number between 0 to 10

function whole(){
    let a = Math.floor(Math.random()*10);
    return a;
}
console.log(whole())

#2. write a function which will get two parameters and return a sum of it

function sum(a,b){
    return (a+b);
}

let a =5;
let b=10;
console.log(sum(a,b));

#3. write a program to create a calculator with all airthematic operations has functions

function calculator_sum(a,b){
    return (a+b);
}

function calculator_sub(a,b){
    return (a-b);
}

function calculator_mul(a,b){
    return (a*b);
}

function calculator_div(a,b){
    return (a/b);
}

function calculator_mod(a,b){
    return (a%b);
}

let a=10;
let b=5;
console.log(calculator_sum(a,b));
console.log(calculator_sub(a,b));
console.log(calculator_mul(a,b));
console.log(calculator_div(a,b));
console.log(calculator_mod(a,b));

